{
  "name": "jquery-injector",
  "version": "0.3",
  "manifest_version": 2,
  "description": "Injects jquery to current page.",
  "background": {
    "scripts": ["background.js"],
    "persistent" : false
  },
  "browser_action": {
    "default_icon": "jquery_icon.png",
    "default_title": "just click to inject"
  },
  "options_page": "options.html",
  "icons": {
    "16": "jquery.png",
    "48": "jquery.png",
    "128": "jquery.png",
    "256": "jquery.png"
  },
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'",
  "permissions": [
    "alarms",
    "tabs",
    "https://*/*",
    "http://*/*"
  ]
}
